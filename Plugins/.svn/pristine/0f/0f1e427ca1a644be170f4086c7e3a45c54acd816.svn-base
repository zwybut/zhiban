/**可编辑输入框样式***********************************************begin*/
/*input输入框样式*/
.editInput_input{
    text-align: center;
    line-height:22px;
    font-size: 12px;
    color: #292f33;
    border:1px solid transparent;
    background: transparent;
}

.editInput_input.active{
    background-color: #ffffff;
    border-color:#dce0e7;
}
/*进行编辑按钮样式*/
.editInput_edit{
    vertical-align: middle;
    margin-left: 10px;
    display: inline-block;
    width:13px;
    height:13px;
    background: url("images/confirm.png") no-repeat left 0;
}
/*确认信息输入按钮样式*/
.editInput_submit{
    vertical-align: middle;
    margin-left: 10px;
    display:none;
    width:13px;
    height:13px;
    background: url("images/confirm.png") no-repeat right 0;
}

/**可编辑输入框样式***********************************************end*/

/**确认框样式***********************************************begin*/
.confirmWin_mask{
    display: none;
    position: absolute;
    top:0;
    left:0;
    right:0;
    bottom:0;
    background-color: black;
    opacity: 0.4;
    -moz-opacity:0.4;
    -khtml-opacity:0.4;
    filter: alpha(opacity=40);
    z-index: 999;
}
.confirmWin_winFrame {
    display: none;
    position: absolute;
    width: 410px;
    height: 127px;
    border-radius: 2px;
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    -o-border-radius: 2px;
    background-color: #ffffff;
    z-index: 1000;
}
.confirmWin_closeIcon{
    float: right;
    width:10px;
    height:10px;
    margin-top: 20px;
    margin-right:20px;
    background: url("images/closeIcon.png")no-repeat center;
    cursor: pointer;
}
.confirmWin_titleDiv:after{
    content: '';
    display: block;
    clear: both;
    height: 0;
    font-size: 0;
    *zoom:1;
}
.confirmWin_title{
    margin-left: 20px;
    font-size: 14px;
    font-weight:bold;
    letter-spacing: 1px;
    color: #2c2c31;
    line-height:40px;
    float: left;
}
.confirmWin_content{
    padding:0 20px;
}
.confirmWin_content input{
    width: 398px;
    height: 28px;
    border: 1px solid #eee;
    padding: 0 30px 0 10px;
    margin-bottom: 10px;
    background: url(images/pen_1_02.png)no-repeat 410px center;
}
.confirmWin_content input:hover,.confirmWin_content input:focus{
    border-color: #20cfd5;
    background-image: url(images/pen_1_01.png);
    -webkit-background-image: url(images/pen_1_01.png);
    -moz-background-image: url(images/pen_1_01.png);
}
.confirmWin_btnDiv{
    position: absolute;
    right: 0;
    bottom: 20px;

}
.confirmWin_enterBtn{
    width: 60px;
    height: 20px;
    display: inline-block;
    position: relative;
    line-height: 20px;
    margin-right: 20px;
    cursor: pointer;
    vertical-align: middle;
    text-align: center;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    /* Support: IE <= 11 */
    overflow: visible;
    background: dodgerblue;
    color: #fff;
    border:1px solid transparent;
    -webkit-border-radius:3px;
    -moz-border-radius:3px;
    border-radius:3px;
    font-size: 12px;
}


.confirmWin_cancleBtn{
    width: 60px;
    height: 20px;
    display: inline-block;
    position: relative;
    line-height: 20px;
    margin-right: 20px;
    cursor: pointer;
    vertical-align: middle;
    text-align: center;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    /* Support: IE <= 11 */
    overflow: visible;
    background-color: #fff;
    font-weight: normal;
    border:1px solid #dde1e8;
    -webkit-border-radius:3px;
    -moz-border-radius:3px;
    border-radius:3px;
    font-size: 12px;
}
.confirmWin_enterBtn:hover{
    background:#00a2d4;
}
.confirmWin_cancleBtn:hover{
    background: #dde1e8;
}
.confirmWin_enterBtn:active{
    -webkit-box-shadow: 0 0 5px rgba(0,0,0,0.3);
    -moz-box-shadow: 0 0 5px rgba(0,0,0,0.3);
    box-shadow: 0 0 5px rgba(0,0,0,0.3);
}
.confirmWin_cancleBtn:active{

}
/**确认框样式***********************************************begin*/
