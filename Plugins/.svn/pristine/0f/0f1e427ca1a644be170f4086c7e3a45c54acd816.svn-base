
/*选择框样式*/
.select_box{ font-size: 12px;color: #fff;line-height: 24px;float: left;margin-right: 8px;margin-top: 18px; position: relative;}
.select_showbox{border: 1px solid transparent;border-radius: 3px;-webkit-border-radius:3px; -moz-border-radius: 3px; -o-border-radius: 3px;height: 24px;padding-left: 8px;background:#389bff url(images/triangleicon_03.png) no-repeat 0 center;}
.select_option{border: 1px solid #dee5e7;border-top: none;display: none;position: absolute;left: 0;top: 26px;z-index: 999;overflow-y: auto}
.select_option li{padding-left: 8px;color: #4c4c4c;background: #fff}
.select_option li.selected{background-color: #F3F3F3;color: #4c4c4c;}
.select_option li.hover{background: #389bff; color: #fff;}


/*冻结表格样式*/
.t th{
    font-size:12px;
    color: #000;
    text-align:center;
    background:#fafafa;
    vertical-align:middle;
    line-height:40px;
}

.t td{
    font-size:12px;
    color: #000;
    text-align:center;
    line-height:40px;
}
.table_fixhead{ position:absolute; left:0; height:40px; top:0;right:17px; overflow:hidden;z-index: 1}
.table_fixLeft{ position:absolute; left:0; top:40px; bottom:17px; overflow-y:hidden;z-index: 8;background: #fff}
.table_fixheadLeft{
    position: absolute;
    left: 0;
    top:0;
    z-index: 10;
}

.t tbody tr.hover{
    background:#dbedff ;
}
.t tbody tr.hover td{
    border-color: #dbedff;
}
.t tbody tr.active{
    background:#389bff ;
    filter:drop-shadow(0 3px 3px #908f8f);
    -webkig-filter:drop-shadow(0 3px 3px #908f8f);
    -moz-filter:drop-shadow(0 3px 3px #908f8f);
}

.t tbody tr.active td{
    border-color: #389bff;
    color: #fff;
}
.table_fixLeft  tr.hover{
    background:#dbedff ;
}
.table_fixLeft  tr.hover td{
    border-color: #dbedff;
}
.table_fixLeft tr.active{
    background:#389bff ;
    filter:drop-shadow(0 3px 3px #908f8f);
    -webkig-filter:drop-shadow(0 3px 3px #908f8f);
    -moz-filter:drop-shadow(0 3px 3px #908f8f);
}
.table_fixLeft tr.active td{
    border-color: #389bff;
    color: #fff;
}

